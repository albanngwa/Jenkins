/* groovylint-disable-next-line CompileStatic */
pipeline {
    agent any

    stages {
        stage('Build Docker image') {
            steps {
                sh 'docker build -f ci/Dockerfile -t my-playwright .'
                sh 'docker build -f ci/Dockerfile/Dockerfile-aws-cli -t my-aws-cli .'
            }
        }
    }
}
